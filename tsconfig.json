{
  "include": [
    "src/**/*",
  ],
  "exclude": ["node_modules", "**/*.spec.ts"],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "types": [
      "@league-of-foundry-developers/foundry-vtt-types"
    ],
    "moduleResolution": "node",
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "noImplicitOverride": true
  }
}